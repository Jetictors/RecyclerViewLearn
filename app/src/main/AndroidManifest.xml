<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.features.moneies.article.recyclerviewlearn">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.layoutManagerType.LayoutTypeActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.layoutManagerType.LinearManagerActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.layoutManagerType.GridManagerActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.moreItem.MoreItemTypeActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.moreItem.CopyAlipayIndexActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.moreItem.CopyAlipayFoodActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>